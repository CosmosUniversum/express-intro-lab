<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Constellation Mini Database</title>
</head>
<body>
  <h1>A list of Constellations</h1>
  <% if(error) { %> 
    <h2><%= error %> </h2>
  <% } %>
  <ul>
    <% constellation.forEach(function(constellation){ %>
      <li>
        <%= constellation.name %> 
      -
      <%= constellation.squareDegrees %> 
      -
      <%= constellation._id  %> 
      </li>
    <% }) %> 
  </ul>
</body>
</html>